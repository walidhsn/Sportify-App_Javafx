<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" stylesheets="@../resources/styles.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.EquipmentList">
   <TableView fx:id="equipmentTable" layoutX="98.0" layoutY="65.0" prefHeight="390.0" prefWidth="604.0" stylesheets="@../resources/styles.css">
      <columns>
         <TableColumn fx:id="nameColumn" prefWidth="120.0" text="Name" />
         <TableColumn fx:id="categoryColumn" prefWidth="120.0" text="Category" />
         <TableColumn fx:id="quantityColumn" prefWidth="120.0" text="Quantity" />
         <TableColumn fx:id="priceColumn" prefWidth="120.0" text="Price" />
         <TableColumn fx:id="SupplierColumn" prefWidth="120.0" text="Supplier" />
      </columns>
   </TableView>
   <Button fx:id="btnAdd" layoutX="328.0" layoutY="467.0" mnemonicParsing="false" onAction="#handleAddButtonClick" prefHeight="42.0" prefWidth="128.0" stylesheets="@../resources/button.css" text="Add" textFill="RED">
      <font>
         <Font size="20.0" />
      </font>
   </Button>
   <TextField fx:id="bar" layoutX="319.0" layoutY="14.0" stylesheets="@../resources/styles.css" />
   <Button fx:id="reherche" layoutX="483.0" layoutY="13.0" mnemonicParsing="false" onAction="#search" prefHeight="38.0" prefWidth="98.0" stylesheets="@../resources/button.css" text="search" />
   <Button fx:id="Excel" layoutX="646.0" layoutY="476.0" mnemonicParsing="false" onAction="#AjouterExcel" stylesheets="@../resources/button.css" text="Exel" />
   <Button fx:id="pdf" layoutX="567.0" layoutY="476.0" mnemonicParsing="false" onAction="#Ajouterpdf" stylesheets="@../resources/button.css" text="pdf" />
</AnchorPane>
